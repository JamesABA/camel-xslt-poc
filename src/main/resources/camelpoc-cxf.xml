<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:cxf="http://camel.apache.org/schema/cxf"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://camel.apache.org/schema/cxf http://camel.apache.org/schema/cxf/camel-cxf.xsd">

    <!-- CXF (Webservice) Endpoint configuration -->

    <cxf:cxfEndpoint id="contactUpdateWS"
                     address="http://localhost:8090/camelxsltpoc/ws/ContactUpdate"
                     endpointName="s:ContactUpdatePort"
                     serviceName="s:ContactUpdate"
                     wsdlURL="wsdl/ContactUpdate.wsdl"
                     xmlns:s="http://webservices.poc.businessagility.com/"

    />


    <cxf:cxfEndpoint id="CamelPocWS"
                     address="http://localhost:8090/camelxsltpoc/ws/CamelPoc"
                     endpointName="s:CamelPocPort"
                     serviceName="s:CamelPocService"
                     wsdlURL="wsdl/CamelPoc.wsdl"
                     xmlns:s="http://camelpoc.poc.businessagility.com"

    />

</beans>

